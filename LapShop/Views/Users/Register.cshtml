@model UserModel










<!-- breadcrumb start -->
<div class="breadcrumb-section">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <div class="page-title">
                    <h2>create account</h2>
                </div>
            </div>
            <div class="col-sm-6">
                <nav aria-label="breadcrumb" class="theme-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">create account</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<!-- breadcrumb End -->
<!--section start-->
<section class="register-page section-b-space">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h3>create account</h3>
                <div class="theme-card">
                    <form class="theme-form" method="post" asp-controller="Users" asp-action="Register">
                        <div class="form-row">
                            <div asp-validation-summary="All">

                            </div>
                            <div class="col-md-6">
                                <input asp-for="ReturnUrl" type="hidden" />

                                <label for="email">First Name</label>
                                <input asp-for="FirstName" type="text" class="form-control" id="fname"
                                    placeholder="First Name" required="">
                                <span asp-validation-for="FirstName"></span>
                            </div>
                            <div class="col-md-6">
                                <label for="review">Last Name</label>
                                <input asp-for="LastName" type="text" class="form-control" id="lname"
                                    placeholder="Last Name" required="">
                                <span asp-validation-for="LastName"></span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-md-6">
                                <label for="email">email</label>
                                <input asp-for="Email" type="text" class="form-control" id="email" placeholder="Email"
                                    required="">
                                <span asp-validation-for="Email"></span>
                            </div>
                            <div class="col-md-6">
                                <label for="review">Password</label>
                                <input asp-for="Password" type="password" class="form-control" id="review"
                                    placeholder="Enter your password" required="">
                                <span asp-validation-for="Password"></span>
                            </div>
                            <button type="submit" class="btn btn-solid">create Account</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--Section ends-->

<div class="w-full bg-white flex flex-col rounded-2xl p-4 gap-6 min-h-screen ">

    <partial name="_Breadcrumb" />

    <section class="w-full min-h-full pt-12 flex flex-col justify-center items-center gap-6">
        <div class="text-center space-y-1.5">
            <h3 class="text-2xl font-semibold">Register Now and Start Exploring!</h3>
            <p class="text-xs">Please fill in your details to create an account</p>
        </div>
        <form method="post" asp-controller="Users" asp-action="Register"
            class="flex flex-col  w-full justify-center items-center gap-6 ">
            <div class="flex  w-full max-w-xl justify-center items-center gap-3 ">
                <div class="w-full">
                    <label for="Fname" class="block mb-2 text-md font-medium text-gray-900 ">
                        First Name
                    </label>
                    <div class="relative text-center ">
